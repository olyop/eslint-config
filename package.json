{
	"license": "MIT",
	"private": false,
	"type": "commonjs",
	"main": ".eslintrc.js",
	"version": "6.7.0",
	"name": "@oly_op/eslint-config",
	"description": "@oly_op/eslint-config",
	"bugs": "https://github.com/olyop/eslint-config/issues",
	"homepage": "https://github.com/olyop/eslint-config/blob/master/README.md",
	"author": {
		"name": "Oliver Plummer",
		"email": "oliver.plummer@outlook.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/olyop/eslint-config.git"
	},
	"files": [
		".eslintrc.js"
	],
	"volta": {
		"node": "21.2.0",
		"npm": "10.2.4"
	},
	"scripts": {
		"clean": "rimraf .eslintrc.js eslint-config.js",
		"compile-to-cjs": "node -e \"const fs = require('fs'), filename = 'eslint-config.js'; fs.writeFileSync(filename, fs.readFileSync(filename).toString().replace('exports.default', 'module.exports'));\"",
		"compile": "tsc && npm run compile-to-cjs",
		"rename": "node -e \"require('fs').renameSync('eslint-config.js', '.eslintrc.js');\"",
		"build": "npm run clean && npm run compile && npm run rename",
		"prepublishOnly": "npm run build"
	},
	"peerDependencies": {
		"@typescript-eslint/eslint-plugin": "7.0.2",
		"@typescript-eslint/parser": "7.0.2",
		"eslint": "8.57.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "17.1.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-json": "3.1.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-unicorn": "51.0.1"
	},
	"devDependencies": {
		"@oly_op/cspell-dict": "1.1.5",
		"@oly_op/prettier-config": "1.8.2",
		"@oly_op/tsconfig": "1.2.0",
		"@types/node": "20.11.20",
		"@typescript-eslint/eslint-plugin": "7.0.2",
		"@typescript-eslint/parser": "7.0.2",
		"@typescript-eslint/utils": "7.0.2",
		"eslint": "8.57.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "17.1.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-json": "3.1.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-unicorn": "51.0.1",
		"npm-check-updates": "16.14.15",
		"rimraf": "5.0.5"
	},
	"overrides": {
		"eslint-config-airbnb-typescript": {
			"@typescript-eslint/eslint-plugin": "7.0.2",
			"@typescript-eslint/parser": "7.0.2"
		}
	}
}
